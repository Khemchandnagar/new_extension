{
    "manifest_version": 2,
    "name": "Face Login",
    "version": "0.0.1",
    "content_security_policy": "script-src 'self' blob: filesystem: https://cdn.jsdelivr.net 'wasm-eval'; object-src 'self' script-src 'self' 'unsafe-eval';",
    "permissions": [
        "tabs",
        "activeTab",
        "https://ui.nupins.in/login",
        "https://www.website.com/webmail/sign-in/",
        "https://admin.nupins.in/",
        "https://www.instagram.com/accounts/login/",
        "https://codeforces.com/enter",
        "https://www.codechef.com/login"
    ],

    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },

    "browser_action": {
        "default_popup": "popup.html"
    }
}